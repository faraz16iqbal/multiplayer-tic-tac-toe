{
  "name": "tic-tac-toe",
  "version": "1.0.0",
  "description": "Multiplayer Tic-Tac-Toe Game",
  "main": "server.js",
  "types": "commonjs, module",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon --exec babel-node backend/server",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Faraz Iqbal",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/node": "^7.14.2",
    "@babel/preset-env": "^7.14.2",
    "@types/socket.io-client": "^3.0.0",
    "concurrently": "^6.2.0",
    "nodemon": "^2.0.7"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "qs": "^6.7.0",
    "socket.io": "^4.1.2",
    "uuid": "^8.3.2"
  }
}
